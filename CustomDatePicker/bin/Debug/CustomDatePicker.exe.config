<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>

  <appSettings>
    
    <!--The date format is configurable. Below are the possible values
    dd/MM/yyyy
    yyyy/MM/dd
    dd-MM-yyyy
    MM-dd-yyyy
    dd.MM.yyyy
    MM.dd.yyyy
    
    Please note : the date separator value also has to be updated as per the date format.-->
    
    <add key="DateFormat" value="MM-dd-yyyy"/>
    <add key="DateSepator" value="-" />
  
  </appSettings>
</configuration>